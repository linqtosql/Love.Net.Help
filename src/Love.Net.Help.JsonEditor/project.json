    "owners": [
      "rigofunc",
      "rigofunc@outlook.com"
    ],
    "licenseUrl": "https://github.com/lovedotnet/Love.Net.Help/LICENSE",
    "projectUrl": "https://github.com/lovedotnet/Love.Net.Help",
    "repository": {
      "type": "git",
      "url": "https://github.com/lovedotnet/Love.Net.Help.git"
    }
  },
  "buildOptions": {
    "optimize": true,
