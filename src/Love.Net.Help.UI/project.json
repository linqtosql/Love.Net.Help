{
  "version": "1.0.0-*",
  "authors": [ "rigofunc", "rigofunc@outlook.com", "xuyingting" ],
  "packOptions": {
    "summary": "A swagger style UI for Love.Net.Help - a API toolchain for ASP.NET Core.",
    "tags": [
      "documentation",
      "discovery",
      "help",
      "webapi",
      "aspnet",
      "aspnetcore"
    ],
    "owners": [
      "rigofunc",
      "rigofunc@outlook.com"
    ],
    "licenseUrl": "https://github.com/lovedotnet/Love.Net.Help/LICENSE",
    "projectUrl": "https://github.com/lovedotnet/Love.Net.Help",
    "repository": {
      "type": "git",
      "url": "https://github.com/lovedotnet/Love.Net.Help.git"
    }
  },
  "buildOptions": {
    "optimize": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "warningsAsErrors": true,
    "embed": {
      "include": [
        "UI/dist/**/*.*"
      ],
      "exclude": [
        "UI/dist/**/*.map"
      ]
    }
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.Extensions.FileProviders.Embedded": "1.0.0",
    "Love.Net.Help": "1.0.0-*"
  },

  "frameworks": {
    "netstandard1.6": {
    }
  }
}